<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SynchronizedLinearLayout">
        <attr name="synchronizedView" format="reference" />
        <attr name="placeholderView" format="reference" />
        <attr name="syncView_gravity">
            <enum name="left" value="0" />
            <enum name="center_horizontal" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="SynchronizedRelativeLayout">
        <attr name="synchronizedView" />
        <attr name="placeholderView" />
        <attr name="syncView_gravity" />
    </declare-styleable>
</resources>